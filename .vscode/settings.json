{
    "typescript.check.workspaceVersion": false ,
    
    
    
 // Configure glob patterns for excluding files and folders.
  "files.exclude": {
    "**/.git": true,
    "**/.svn": true,
    "**/.hg": true,
    "**/.DS_Store": true,
        "**/*.js": {"when": "$(basename).ts"},
        "**/*.*.map": true
  }
}